/*
Sample Input  :
3
Sample Output  :
1=1
1+2=3
1+2+3=6


*/

import java.util.*;
public class SumPattern {
	
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		 int sum=0;
		for(int i=1;i<=n;i++)
		{
		    	 sum=0;
			for(int j=1;j<=i;j++)
			{
				System.out.print(j);
				if(i>1 && i!=j)
				{
					System.out.print("+");
				}
				sum=sum+j;
			}
			System.out.print("="+sum);
			System.out.println();
		}
	}
}
